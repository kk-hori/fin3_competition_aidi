{
    "directories": {
        "input": "../input",
        "output": "../output"
    },
    "rules": {
        "max_tokens_answer": 54,
        "docs_num": 19
    },
    "az_ai_document_intelligence": {
        "model_id": "prebuilt-layout",
        "output_content_format": "markdown"
    },
    "azure_openai": {
        "embedding": {
            "model_name": "text-embedding-3-large",
            "max_tokens": 8000
        }
    },
    "elasticsearch": {
        "url": "http://localhost:9200",
        "data": {
            "path": "../data/elasticsearch/data",
            "index": {
                "name": "documents",
                "dims_embedding": 3072
            }
        }
    }
}
